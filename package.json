{
  "name": "node-btc-trace",
  "version": "2.1.0",
  "description": "An open-source, developer-friendly REST API built with Node.js and Express to explore Bitcoin blockchain data like blocks and chain status.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "stage": "cross-env NODE_ENV=stage node server.js"
  },
  "keywords": [
    "bitcoin",
    "btc",
    "bitcoin-core",
    "bitcoin-rpc",
    "json-rpc",
    "blockchain",
    "nodejs",
    "express",
    "swagger",
    "bitcoin-api",
    "bitcoin-blockchain",
    "crypto",
    "cryptocurrency",
    "btc-rpc",
    "btc-node",
    "bitcoin-node",
    "bitcoin-data"
  ],
  "author": "Liam Morris",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.4",
    "axios-retry": "^4.5.0",
    "change-case": "^5.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "joi": "^17.13.3",
    "morgan": "^1.10.0",
    "pino": "^9.7.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "read-pkg": "^9.0.1",
    "swagger-jsdoc": "^6.2.8",
